#include <bits/stdc++.h>
using namespace std;

void print(vector<int> v)
{
    for(int i=0;i<v.size();i++)
    {
        cout << v[i]<< " ";
    }
    cout << "\n";
}

vector<int> getInput()
{
    int n;
    cin >> n;
    int temp;
    vector<int> v;
    for(int i=0;i<n;i++)
    {
        cin >> temp;
        v.push_back(temp);
    }
    return v;
}

int main()
{
    vector<int> v;
    v = getInput();
    int n = v.size();
    int i=0;
    int j=n-1;
    
    // if i is even then increment i
    // if j is odd then decrement j
    // if i is odd and j is even then swap
    
    while(j>i)
    {
        if(v[i]%2==0)
        {
            i = i + 1;
        }
        else
        if(v[j]%2==1)
        {
            j = j - 1;
        }
        else
        if(v[i]%2==1 && v[j]%2==0)
        {
            int temp;
            temp = v[i];
            v[i] = v[j];
            v[j] = temp;
        }
    }
    print(v);
}

